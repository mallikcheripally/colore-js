{
  "name": "colore-js",
  "version": "1.3.1",
  "description": "A JS library for Color Conversions, Manipulations, Harmony Generations, Accessibility Analysis and Parsing.",
  "repository": {
    "type": "git",
    "url": "https://github.com/mallikcheripally/colore-js.git"
  },
  "keywords": [
    "color",
    "manipulation",
    "conversion",
    "javascript",
    "typescript",
    "colore-js",
    "colore",
    "css",
    "rgb",
    "hsl",
    "hex",
    "contrast",
    "accessibility",
    "a11y",
    "color blending",
    "color conversion",
    "color harmony",
    "color manipulation",
    "css color",
    "color interpolation"
  ],
  "author": "Mallik Cheripally",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mallikcheripally/colore-js/issues"
  },
  "homepage": "https://github.com/mallikcheripally/colore-js#readme",
  "type": "module",
  "main": "dist/colore.cjs.js",
  "module": "dist/colore.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "format": "prettier --write 'src/**/*.{js,ts}'",
    "release": "standard-version",
    "prepublishOnly": "yarn build",
    "test": "jest",
    "analyze-code": "source-map-explorer dist/colore.cjs.js",
    "publish": "npm publish --access=public"
  },
  "devDependencies": {
    "@eslint/js": "^9.3.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "9.x",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "globals": "^15.3.0",
    "jest": "^29.7.0",
    "jest-module-name-mapper": "^0.1.5",
    "lint-staged": "^15.2.4",
    "prettier": "^3.2.5",
    "rollup": "^4.18.0",
    "source-map-explorer": "^2.5.3",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.3",
    "typescript": "^5.4.5"
  }
}
